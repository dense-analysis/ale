After:
  call ale#linter#Reset()

Given scala(An empty Scala file):
Execute(The handler should return the correct scalac command and executable):
  AssertEqual 'scalac', ale#scala#GetExecutableForLinter(bufnr(''), 'scalac')
  AssertEqual
  \ ale#Escape('scalac') . ' -Ystop-after:parser %t',
  \ ale#scala#GetCommandForLinter(bufnr(''), 'scalac')

Given scala(An empty Scala file):
Execute(The handler should return the correct fsc command and executable):
  AssertEqual 'fsc', ale#scala#GetExecutableForLinter(bufnr(''), 'fsc')
  AssertEqual
  \ ale#Escape('fsc') . ' -Ystop-after:parser %t',
  \ ale#scala#GetCommandForLinter(bufnr(''), 'fsc')

Given scala.sbt(An empty SBT file):
Execute(The fsc handler should return an empty executable for sbt files):
  AssertEqual '', ale#scala#GetExecutableForLinter(bufnr(''), 'fsc')

Given scala.sbt(An empty SBT file):
Execute(The scalac handler should return an empty executable for sbt files):
  AssertEqual '', ale#scala#GetExecutableForLinter(bufnr(''), 'scalac')
